
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <table>
            @for (int i = 0; i < 3; i++)
            {
                <tr>
                    @for (int j = 0; j < 4; j++)
                {
                    <td>Cell @(i * 4 + j + 1)</td>
                }
                </tr>
            }
        </table>
    </div>
</body>
</html>
