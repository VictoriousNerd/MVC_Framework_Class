
@{
    Layout = null;
    var a = 0;
    var b = 1;
    var c = 0;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <p>@a</p>
        <p>@b</p>
        @for (var i = 1; i < 10; i++)
        {
            c = (a + b);
            <p>@c</p>
            a = b;
            b = c;
        }
    </div>
</body>
</html>
